{
  "type": "module",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "concurrently": "^8.2.1",
    "eslint-import-resolver-typescript": "^3.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "clean": "rimraf -g ./build/* && rimraf -g ./docs/* && rimraf -g ./docs/.*",
    "build": "rimraf -g ./build/* && tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "update-import": "npx pnpm remove dice-pool-calc && npx pnpm add dice-pool-calc"
  },
  "dependencies": {
    "dice-pool-calc": "1.0.0-alpha.0",
    "immutable": "5.0.0-beta.2"
  }
}
